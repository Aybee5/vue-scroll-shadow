{
  "name": "vue-scroll-shadow-v3",
  "description": "omponent that adds shadow to scrollable area works both with vertical and horizontal scrolling",
  "version": "0.1.3",
  "type": "module",
  "main": "./dist/vue-scroll-shadow.umd.cjs",
  "module": "./dist/vue-scroll-shadow.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-scroll-shadow.js",
      "require": "./dist/vue-scroll-shadow.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && vue-tsc --declaration --emitDeclarationOnly --declarationDir ./dist",
    "preview": "vite preview",
    "generate:types": ""
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  },
  "keywords": [
    "vue",
    "vue 3",
    "scroll",
    "shadow",
    "scroll-shadow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Aybee5/vue-scroll-shadow"
  }
}
